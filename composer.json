{
    "name": "khaertel/laravel-translation-manager",
    "description": "Manage Laravel Translations",
    "keywords": ["laravel", "translations", "translator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Klaus Haertel",
            "email": "klaus@signalq.ca"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.x|5.1.x",
        "illuminate/translation": "5.0.x|5.1.x",
        "symfony/finder": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Barryvdh\\TranslationManager\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "minimum-stability": "stable"
}
